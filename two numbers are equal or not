#include <iostream>
using namespace std;

int main() {
  float num1, num2;
  cout << "Enter 1st number: ";
  cin >> num1;
  cout << "Enter 2nd number: ";
  cin >> num2;

  if (num1 == num2) {
    cout << "Both numbers are equal." << endl;
  } else {
    cout << "Numbers are not equal." << endl;
  }

  return 0;
}

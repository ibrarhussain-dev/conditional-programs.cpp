#include <iostream>
using namespace std;

int main() {
  float num1, num2, num3, average;
      cout << "Enter 1st number: ";
  cin >> num1;
      cout << "Enter 2nd number: ";
  cin >> num2;
      cout << "Enter 3rd number: ";
  cin >> num3;

  average = (num1 + num2 + num3) / 3.0;
      if(average>=80)
      cout<<"you are above standard"<<endl;
    cout<<"admission granted"<<endl;
    return 0;
}

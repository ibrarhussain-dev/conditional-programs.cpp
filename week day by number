#include <iostream>
using namespace std;

int main() {
    int day;
    cout<<"enter day number of the week : ";
    cin>>day;
    if(day==1){
        cout<<"Monday"<<endl;
    }
    else if(day==2){
        cout<<"tuesday"<<endl;
    }
    else if(day==3){
        cout<<"wednesday"<<endl;
    }
    else if(day==4){
        cout<<"thursday"<<endl;
    }
    else if(day==5){
        cout<<"friday"<<endl;
    }
    else if(day==6){
        cout<<"satureday"<<endl;
    }
    else if(day==7){
        cout<<"Sunday"<<endl;
    }
    else
        cout<<"invaild day number";
}

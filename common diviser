#include <iostream>
using namespace std;
int main(){
    int a, b, c;
    cout<<"enter value of a :";
    cin>>a;
    cout<<"enter value of b :";
    cin>>b;
    cout<<"enter value of c :";
    cin>>c;
    if(b%a==0 && c%a==0){
        cout<<"a is common diviser of b and c";
    }
    else
        cout<<"a is not common diviser";
}

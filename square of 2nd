#include <iostream>
using namespace std;

int main() {
  float num1, num2;
  cout << "Enter 1st number: ";
  cin >> num1;
  cout << "Enter 2nd number: ";
  cin >> num2;

  if (num1*num1 == num2) {
    cout << "The 2nd number is the square of the 1st number." << endl;
  } else {
    cout << "The 2nd number is not the square of the 1st number." << endl;
  }

  return 0;
}

#include <iostream>
using namespace std;

int main() {
  float num1, num2, num3, max;
      cout << "Enter 1st number: ";
  cin >> num1;
      cout << "Enter 2nd number: ";
  cin >> num2;
      cout << "Enter 3rd number: ";
  cin >> num3;

  max = num1;
  if(num2 > max) {
    max = num2;
  }
  if(num3 > max) {
    max = num3;
  }

  cout << "The maximum number is: " << max << endl;

  return 0;
}

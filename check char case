#include<iostream>
using namespace std;
int main(){
    char ch;
    cout<<"enter a charater : ";
    cin>>ch;
    if(ch>='a'  &&  ch<='z'){
        cout<<"lower case letter"<<endl;
    }
    else if(ch>='A'  &&  ch<='Z'){
        cout<<"upper case letter"<<endl;
    }
    else if(ch>='0'  &&  ch<='9'){
        cout<<"its a number"<<endl;
    }
    else
    cout<<"special charater"<<endl;
}

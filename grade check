#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter nmarks : ";
    cin >> n;
    if (n >= 90) 
        cout << "Grade A" << endl;
    else if (n >= 80) 
        cout << "Grade B" << endl;
    else if (n >= 70) 
        cout << "Grade C" << endl;
    else if (n >= 60)
        cout << "Grade D" << endl;
    else 
        cout << "Grade F" << endl;
    return 0;
}
